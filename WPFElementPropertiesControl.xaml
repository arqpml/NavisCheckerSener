<UserControl x:Class="NavisCheckerSener.WPFElementPropertiesControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:NavisCheckerSener"
             mc:Ignorable="d" 
             d:DesignHeight="380" d:DesignWidth="450">

    <UserControl.Resources>
        <local:Models x:Key="models"/>
    </UserControl.Resources>

    <Grid DataContext="{StaticResource ResourceKey=models}">
        <ListView ItemsSource="{Binding Information}" Margin="0,0,20,0">
            
        </ListView>
        
     
    </Grid>
</UserControl>
